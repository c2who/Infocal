                <listEntry value="30">@Strings.stress</listEntry>
                <!-- no bodybatterystress -->
                <listEntry value="39">@Strings.timetorecovery</listEntry>
                <listEntry value="4">@Strings.movebar</listEntry>
                <listEntry value="16">@Strings.groupnoti</listEntry>        <!-- Phone -->
                <listEntry value="11">@Strings.alarm</listEntry>
                <listEntry value="20">@Strings.phonestatus</listEntry>
                <listEntry value="10">@Strings.notification</listEntry>
                <listEntry value="5">@Strings.steps</listEntry>
                <listEntry value="13">@Strings.temperature</listEntry>      <!-- Temperature -->
                <listEntry value="32">@Strings.temperature_garmin</listEntry>
                <listEntry value="23">@Strings.temperature_out</listEntry>
                <listEntry value="24">@Strings.temperature_hl</listEntry>
                <listEntry value="35">@Strings.temperature_hl_garmin</listEntry>
                <listEntry value="33">@Strings.precipitation_garmin</listEntry>
                <listEntry value="8">@Strings.time</listEntry>              <!-- Time -->
                <listEntry value="26">@Strings.ampm</listEntry>
                <listEntry value="19">@Strings.time_secondary</listEntry>
                <listEntry value="14">@Strings.sunriseset</listEntry>
                <listEntry value="25">@Strings.weather</listEntry>          <!-- Weather -->
                <listEntry value="34">@Strings.weather_garmin</listEntry>
                <listEntry value="28">@Strings.wind</listEntry>
            </settingConfig>
