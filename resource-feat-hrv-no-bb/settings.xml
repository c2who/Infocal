        <setting propertyKey="@Properties.comp12h" title="@Strings.comp12h">
            <settingConfig type="list">
                <listEntry value="9">@Strings.empty</listEntry>
                <listEntry value="6">@Strings.active</listEntry>            <!-- Active Minutes -->
                <listEntry value="36">@Strings.moderate</listEntry>
                <listEntry value="37">@Strings.vigorous</listEntry>
                <listEntry value="38">@Strings.airquality</listEntry>       <!-- Air -->
                <listEntry value="18">@Strings.barometer</listEntry>
